    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/user_navigation"
    app:startDestination="@id/user_navigation_home">

    <fragment
        android:id="@+id/user_navigation_home"
        android:name="com.example.badmintonbookingapp.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/user_navigation_booking"
        android:name="com.example.badmintonbookingapp.ui.booking.BookingFragment"
        android:label="Booking"
        tools:layout="@layout/fragment_booking" />

    <fragment
        android:id="@+id/user_navigation_order"
        android:name="com.example.badmintonbookingapp.ui.order.OrderFragment"
        android:label="Order"
        tools:layout="@layout/fragment_order" />
    
